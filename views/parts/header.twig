<header class="header">
    {% block header %}
        <div class="wrapper">
            <h1 class="hdr-logo" role="banner">
                <a class="hdr-logo-link" href="{{site.url}}" rel="home">{{site.name}}</a>
            </h1>
            <nav id="nav-main" class="nav-main" role="navigation">
                {% include "parts/primary-menu.twig" with {'items': primary_menu.get_items} %}
            </nav><!-- #nav -->
        </div>
    {% endblock %}
</header>
