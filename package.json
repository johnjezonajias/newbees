{
  "name": "newbees",
  "version": "1.0.0",
  "description": "My timber theme.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "concurrently \"sass --watch assets/scss/styles.scss:assets/css/styles.css\" \"npx tailwindcss -i ./assets/css/tailwind.css -o ./dist/css/style.css --watch\"",
    "build": "sass assets/scss/styles.scss assets/css/styles.css && NODE_ENV=production npx tailwindcss -i ./assets/css/tailwind.css -o ./dist/css/style.css --minify"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "^10.4.20",
    "tailwindcss": "^3.4.15"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "postcss-import": "^16.1.0",
    "sass": "^1.82.0"
  }
}
